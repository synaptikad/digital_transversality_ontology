@prefix base: <http://www.synaptik.tech/smart_building#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

base:Actuator a owl:Class ;
    rdfs:label "Actuator" ;
    rdfs:subClassOf base:Equipment .

base:ControlUnit a owl:Class ;
    rdfs:label "Control Unit" ;
    rdfs:subClassOf base:Equipment .

base:Sensor a owl:Class ;
    rdfs:label "Sensor" ;
    rdfs:subClassOf base:Equipment .

base:enables a owl:ObjectProperty ;
    rdfs:label "enables" ;
    rdfs:domain base:Equipment ;
    rdfs:range base:FunctionalCapability .

base:providesData a owl:ObjectProperty ;
    rdfs:label "provides data" ;
    rdfs:domain base:Equipment ;
    rdfs:range base:DataPoint .

base:DataPoint a owl:Class ;
    rdfs:label "Data Point" .

base:FunctionalCapability a owl:Class ;
    rdfs:label "Functional Capability" .

base:Equipment a owl:Class ;
    rdfs:label "Equipment" .

